---
layout: "collection.njk"
---

{{ collections.albumNav | eleventyNavigationBreadcrumb(key) | eleventyNavigationToHtml | safe}}

{% set enhancedAlbums = collections.albumNav | eleventyNavigation(key) | enhanceNavigation(collections.albumNav) %}

{{ content | safe }}

{{ enhancedAlbums.length }} albums

 <div class="image-grid-2">
{% asyncEach album in enhancedAlbums %}
  <div class="h-entry">
  <div>
    <a href="{{ album.url }}">
      {% bigthumb album.featured.value, album.featured.alt, "u-photo image"  %}
    </a>
  </div>
  {{ album.title }}
  </div>
{% endeach %}
</div>
